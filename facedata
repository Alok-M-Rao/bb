create database ReceptionGreet;

use ReceptionGreet;

create table `admin_credentials`(
	`admin_gid` bigint unique,
    `admin_password` varchar(50) not null,
    primary key (`admin_gid`));
    
insert into admin_credentials values('7000017577','Alok');

select * from admin_credentials;

drop table admin_credentials;


create table `employee_details`(
	`employee_gid` bigint unique primary key,
	`name` varchar(50) not null,
    `email` varchar(50) unique not null,
    `dob` date not null,
    `doj` date not null,
    `gender` varchar(10) not null);
    
drop table employee_details;
    
create table `face_mapping`(
	`employee_gid` bigint unique,
    `face_data1` text not null,
    `face_data2` text not null,
    `face_data3` text not null,
    `face_data4` text not null,
    `face_data5` text not null,
    foreign key(employee_gid)
    references employee_details(employee_gid)
    on update cascade on delete cascade);

drop table face_mapping;
    
